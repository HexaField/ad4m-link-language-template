{
  "name": "my-ad4m-language",
  "version": "0.1.0",
  "type": "module",
  "description": "AD4M Link Language template for Deno-based executor",
  "main": "build/index.js",
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf build",
    "test": "vitest run",
    "test:e2e": "node node_modules/@coasys/ad4m-test/build/cli.js --test test/ad4m-e2e.test.js --bundle build/index.js --meta '{\"name\":\"my-ad4m-language\",\"description\":\"My AD4M Language\",\"possibleTemplateParams\":[\"uid\",\"name\"]}'",
    "test:e2e:multi": "node node_modules/@coasys/ad4m-test/build/cli.js --test test/ad4m-e2e.test.js --bundle build/index.js --meta '{\"name\":\"my-ad4m-language\",\"description\":\"My AD4M Language\",\"possibleTemplateParams\":[\"uid\",\"name\"]}' --agents 2"
  },
  "dependencies": {
    "@coasys/ad4m": "^0.8.0"
  },
  "devDependencies": {
    "@coasys/ad4m-test": "^0.11.1",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.0.0",
    "rimraf": "^6.0.0",
    "rollup": "^4.0.0",
    "tslib": "^2.8.0",
    "typescript": "^5.6.0",
    "vitest": "^3.0.0"
  }
}
